        function adminLogout() {
            Swal.fire({
                title: 'Are you want to logout?',
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Logout'
            }).then((result) => {
                if (result.isConfirmed) {
                    location.href = "/admin/logout_admin";
                }
            })
        }

